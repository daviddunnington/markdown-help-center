# Copy this file to .env.local and add your actual Clerk keys
# Get these from https://dashboard.clerk.com/last-active?path=api-keys

# Clerk Authentication (only needed if auth is enabled in config)
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here
CLERK_SECRET_KEY=your_clerk_secret_key_here

# Authentication control
NEXT_PUBLIC_AUTH_ENABLED=true          # Only this needs to be public (for UI)
AUTH_PROTECT_CONTENT=false             # Server-only (removed NEXT_PUBLIC_)
AUTH_PROTECT_EDITOR=true               # Server-only (removed NEXT_PUBLIC_)


# GitHub Integration
GITHUB_TOKEN=your_github_token_here
NEXT_PUBLIC_GITHUB_ENABLED=true
GITHUB_OWNER=github_owner_here
GITHUB_REPO=github_repo_here
GITHUB_BRANCH=branch_name_here